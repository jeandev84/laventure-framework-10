{
  "name": "laventure/framework",
  "description": "MVC framework using php language for building web application",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Laventure\\": "src/"
    }
  },
  "autoload-dev": {
     "psr-4": {
        "PHPUnitTest\\": "tests/phpunit"
     }
  },
  "authors": [
    {
      "name": "Jean-Claude",
      "email": "jeanyao@ymail.com"
    }
  ],
  "require": {
    "php": ">= 8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.4"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
